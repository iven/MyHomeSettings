[color]
    ui = auto
[diff]
    # algorithm = histogram
    # wordregex = ",|.|=|'|\"|(|)|[^[:space:]()='\".,_]+"
    # wordregex = "=|'|\"|[^[:space:]()='\".,]+"
    colorMoved = default
[core]
    excludesfile = ~/.global_ignore
    pager = delta
[interactive]
    diffFilter = delta --color-only
[delta]
    navigate = true
    plus-color = "#356054"
    minus-color = "#503524"
[merge]
    conflictstyle = diff3
[push]
    default = matching
[commit]
    gpgsign = true
[format]
    signOff = true
[rebase]
    autoStash = true
